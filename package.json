{   
	"name": "hover-exec",
	"publisher": "rmzetti",
	"displayName": "Hover Exec",
	"description": "execute codeblocks",
	"version": "0.4.1",
	"repository": "https://github.com/rmzetti/hover-exec",
	"license": "NCSA",
	"contributors": [
		"rmzetti"
	],
    "engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:markdown"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"configuration": {
			"type":"object",
			"title":"Hover Exec",
			"properties": {
				"hover-exec.buddvs":{
					"type":"array",
					"default":[
						"buddvs \"%f\" ",
						"chdir(\"%c\");",
						"//=write('{{'+($1)+'}}')" ],
					"description": "budd script: chdir(\"%c\");"
				},
				"hover-exec.octave":{
					"type":"array",
					"default":[
						"octave \"%f\"",
						"",
						"%%=disp(['{{' $1 '}}'])" ,
						"temp.m" ],
					"description": "octave",
					"startup example":" cd \"%c\"; "
				},
				"hover-exec.matlab":{
					"type":"array",
					"default":[
						"matlab -sd \"%p\" -batch temp",
						"cd '%c';",
						"%%=disp([\"{{\"+($1)+\"}}\"])",
						"temp.m" ],
					"description": "matlab",
					"startup example":"path(path,\"%c\");"
				},
				"hover-exec.scilab":{
					"type":"array",
					"default":[
						"scilex -quit -nb -f \"%f\" ",
						"",
						"//=mprintf('%s\\n','{{'+$1+'}}')" ,
						"temp.sci" ],
					"description": "scilab",
					"startup example":"cd \"%c\";"
				},
				"hover-exec.python":{
					"type":"array",
					"default":[
						"python \"%f\"",
						"",
						"##=print('{{'+str($1)+'}}')" ,
						"temp.py" ],
					"description": "python",
					"startup example":"import os\nos.chdir(\"%c\");"
				},
				"hover-exec.python3":{
					"type":"array",
					"default":[
						"python3 \"%f\" ",
						"",
						"##=print('{{'+str($1)+'}}')" ,
						"temp.py" ],
					"description": "python3",
					"startup example":"import os\nos.chdir(\"%c\");"
				},
				"hover-exec.streamlit1":{
					"type":"array",
					"default":[
						"streamlit run \"%f\" ",
						"","" ,
						"temp.py" ],
					"description": "streamlit"
				},
				"hover-exec.julia":{
					"type":"array",
					"default":[
						"julia \"%f\"",
						"",
						"##=println(string(\"{{\",$1,\"}}\"))" ,
						"temp.jl" ],
					"description": "julia",
					"startup example":"cd(\"%c\")"
				},
				"hover-exec.gnuplot":{
					"type":"array",
					"default":[
						"gnuplot -p -c \"%f\"",
						"",
						"" ,
						"temp.gp" ],
					"description": "for gnuplot no results are returned",
					"startup example":"set loadpath \"%c\""
				},
				"hover-exec.pwsh":{
					"type":"array",
					"default":[
						"pwsh -f \"%f\"",
						"",
						"##='{{'+($1)+'}}'" ,
						"temp.ps1" ],
					"description": "powershell",
					"startup example":"cd \"%c\"; "
				},
				"hover-exec.bash":{
					"type":"array",
					"default":[
						"bash \"%f\"",
						"",
						"##='{{'+($1)+'}}'" ,
						"temp.ps1" ],
					"description": "bash",
					"startup example":"cd \"%c\"; "
				},
				"hover-exec.zsh":{
					"type":"array",
					"default":[
						"pwsh -f \"%f\"",
						"",
						"##='{{'+($1)+'}}'" ,
						"temp.ps1" ],
					"description": "zsh",
					"startup example":"cd \"%c\"; "
				},
				"hover-exec.lua":{
					"type":"array",
					"default":[
						"lua54 \"%f\"",
						"",
						"--=print('{{'..($1)..'}}')" ,
						"temp.lua" ],
					"description": "lua"
				},
				"hover-exec.js":{
					"type":"array",
					"default":[
						"node \"%f\"",
						"",
						"//=console.log('{{'+($1)+'}}')" ,
						"temp.js" ],
					"description": "javascript node",
					"startup example":"process.chdir(\"%c\")"
				},
				"hover-exec.html":{
					"type":"array",
					"default":[
						"\"%f\" ", "", "",
						"temp.html" ],
					"description": "html",
					"comment":"no results are returned"
				},
				"hover-exec.firefox":{
					"type":"array",
					"default":[
						"firefox \"%f\"", "", "",
						"temp.html" ],
					"description": "html",
					"comment":"no results are returned"
				},
				"hover-exec.eval":{
					"type":"array",
					"default":[
						"",	"", 
						"//='{{'+($1)+'}}'" ],
					"description": "eval"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch --devtool eval",
		"package": "webpack --mode production",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.56.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"ts-loader": "^8.3.0",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0",
		"webpack": "^5.40.0",
		"webpack-cli": "^4.7.2"
	},
	"comments": {
		"//": [
			"originally in package:",
			"webpack --mode production --devtool hidden-source-map",
			"for hover, did not need under contributes:",
			"  'capabilities':{'hoverProvider':'true'},"
		]
	}
}
