{   
	"name": "hover-exec",
	"publisher": "rmzetti",
	"displayName": "Hover Exec",
	"description": "execute codeblocks",
	"version": "0.5.0",
	"repository": "https://github.com/rmzetti/hover-exec",
	"license": "NCSA",
	"contributors": [
		"rmzetti"
	],
    "engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:markdown"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{ "command": "hover-exec.exec",
			  "title": "Hover-exec: exec current codeblock"
			}
		],	
		"configuration": {
			"type":"object",
			"title":"Hover Exec",
			"properties": {
				"hover-exec.scripts":{
					"default":{
				"buddvs":{
						"exec":"buddvs \"%f\" ",
						"cmd":"chdir(\"%c\");",
						"inline":"//=",
						"swap":"write('{{'+($1)+'}}')",
						"tempf":"temp.txt"
				},
				"octave":{
						"exec":"octave \"%f\"",
						"cmd":"",
						"inline":"%%=",
						"swap":"disp(['{{' $1 '}}'])",
						"tempf":"temp.m",
						"nb":"in-line comments %, results %%="
				},
				"matlab":{
						"exec":"matlab -sd \"%p\" -batch temp",
						"cmd":"cd '%c'",
						"inline":"%%=",
						"swap":"disp([\"{{\"+($1)+\"}}\"])",
						"tempf":"temp.m",
						"nb":"in-line comments %, results %%="
				},
				"scilab":{
						"exec":"scilex -quit -nb -f \"%f\" ",
						"cmd":"",
						"inline":"//=",
						"swap":"mprintf('%s\\n','{{'+$1+'}}')",
						"tempf":"temp.sci",
						"nb":"in-line comments //, results //="
				},
				"python":{
						"exec":"python \"%f\"",
						"cmd":"",
						"inline":"##=",
						"swap":"print('{{'+str($1)+'}}')",
						"tempf":"temp.py",
						"nb":"in-line comments #, results ##=",
						"prev_cmd":"import os\nos.chdir(\"%c\");"
				},
				"python3":{
						"exec":"python3 \"%f\"",
						"cmd":"",
						"inline":"##=",
						"swap":"print('{{'+str($1)+'}}')",
						"tempf":"temp.py",
						"nb":"in-line comments #, results ##=",
						"prev_cmd":"import os\nos.chdir(\"%c\");"
				},
				"streamlit":{
						"exec":"streamlit run \"%f\" ",
						"cmd":"",
						"inline":"",
						"swap":"",
						"tempf":"temp.py",
						"nb":"in-line comments #, no in-line results"
				},
				"julia":{
						"exec":"julia \"%f\"",
						"cmd":"",
						"inline":"##=",
						"swap":"println(string(\"{{\",$1,\"}}\"))",
						"tempf":"temp.jl",
						"nb":"in-line comments #, results ##=",
						"prev_cmd":"cd(\"%c\")"
				},
				"gnuplot":{
						"exec":"gnuplot -p -c \"%f\"",
						"cmd":"",
						"inline":"",
						"swap":"",
						"tempf":"temp.gp",
						"nb":"no results returned",
						"prev_cmd":"set loadpath \"%c\""
				},
				"pwsh":{
						"exec":"pwsh -f \"%f\"",
						"cmd":"",
						"inline":"##=",
						"swap":"'{{'+($1)+'}}'",
						"tempf":"temp.ps1",
						"nb":"##= for in-line results",
						"prev_cmd":"cd \"%c\"; "
				},
				"bash":{
						"exec":"bash \"%f\"",
						"cmd":"##=",
						"inline":"'{{'+($1)+'}}'",
						"swap":"",
						"tempf":"temp.ps1",
						"nb":"in-line results ##=",
						"prev_cmd":"cd \"%c\";"
				},
				"zsh":{
						"exec":"zsh -f \"%f\"",
						"cmd":"",
						"inline":"##=",
						"swap":"'{{'+($1)+'}}'",
						"tempf":"temp.sh",
						"nb":"in-line results ##=",
						"prev_cmd":"cd \"%c\";"
				},
				"lua":{
						"exec":"lua54 \"%f\"",
						"cmd":"",
						"inline":"--=",
						"swap":"print('{{'..($1)..'}}')",
						"tempf":"temp.lua",
						"nb":"in-line comments --, results --="
				},
				"js":{
						"exec":"node \"%f\"",
						"cmd":"",
						"inline":"//=",
						"swap":"console.log('{{'+($1)+'}}')",
						"tempf":"temp.js",
						"nb":"in-line comments //, results //=",
						"prev_cmd":"process.chdir(\"%c\")"
				},
				"node":{
						"alt":"js"
				},
				"javascript":{
						"alt":"js"
				},
				"html":{
						"exec":"\"%f\"",
						"cmd":"",
						"inline":"",
						"swap":"",
						"tempf":"temp.html",
						"nb":"no results returned"
				},
				"firefox":{
						"exec":"firefox \"%f\"",
						"cmd":"",
						"inline":"",
						"swap":"",
						"tempf":"temp.html",
						"nb":"no results returned",
						"prev_cmd":""
				},				
				"test":{
						"exec":"test_exec",
						"cmd":"",
						"inline":"",
						"swap":"",
						"tempf":"test.tst",
						"nb":"no results returned"
				},
				"eval":{
						"exec":"",
						"cmd":"",
						"inline":"//=",
						"swap":"write('{{',($1),'}}')",
						"swapold":"'{{'+($1)+'}}'",
						"tempf":"temp.js",
						"nb":"in-line comments //, results //=",
						"prev_cmd":""
				},
				"go":{
						"exec":"go run \"%f\"",
						"cmd":"",
						"inline":"//=",
						"swap":"'{{'+($1)+'}}'",
						"tempf":"temp.go",
						"nb":"in-line comments //, results //="
				}
			}}}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch --devtool eval",
		"package": "webpack --mode production",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.56.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"ts-loader": "^8.3.0",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0",
		"webpack": "^5.40.0",
		"webpack-cli": "^4.7.2"
	},
	"comments": {
		"//": [
			"originally in package:",
			"webpack --mode production --devtool hidden-source-map",
			"for hover, did not need under contributes:",
			"  'capabilities':{'hoverProvider':'true'},"
		]
	}
}
